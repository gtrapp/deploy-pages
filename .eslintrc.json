{
    "plugins": ["jest"],
    "extends": ["plugin:github/recommended"],
    "rules": {
      "eslint-comments/no-use": "off",
      "github/no-then": "off",
      "import/no-namespace": "off",
      "no-shadow": "off",
      "no-unused-vars": "off",
      "no-undef": "off"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }